// Grid Graph
digraph {
	"0,0" [label="0,0"]
	"0,1" [label="0,1"]
	"0,2" [label="0,2"]
	"1,0" [label="1,0"]
	"1,1" [label="1,1"]
	"1,2" [label="1,2"]
	"2,0" [label="2,0"]
	"2,1" [label="2,1"]
	"2,2" [label="2,2"]
	"0,1" -> "0,0"
	"0,2" -> "0,1"
	"1,0" -> "0,0"
	"1,1" -> "0,1"
	"1,1" -> "1,0"
	"1,2" -> "0,2"
	"1,2" -> "1,1"
	"2,0" -> "1,0"
	"2,1" -> "1,1"
	"2,1" -> "2,0"
	"2,2" -> "1,2"
	"2,2" -> "2,1"
}
